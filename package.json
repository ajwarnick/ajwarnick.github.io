{
  "name": "warnick",
  "version": "1.0.0",
  "description": "Web Portfolio for Anthony Warnick",
  "scripts": {
    "start": "npm-run-all --parallel watch:sass watch:js serve:eleventy",
    "build": "npm-run-all build:*",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "sass src/_scss/:_site/css",
    "build:js": "rm -rf .parcel-cache && parcel build src/_js/*.js --dist-dir _site/js",
    "watch:sass": "sass --watch src/_scss/:_site/css",
    "watch:js": "rm -rf .parcel-cache && parcel watch src/_js/*.js --dist-dir _site/js",
    "watch:eleventy": "npx @11ty/eleventy --watch",
    "serve:eleventy": "npx @11ty/eleventy --serve",
    "start:eleventy": "npx @11ty/eleventy --serve",
    "debug:eleventy": "DEBUG=* npx @11ty/eleventy",
    "bench:eleventy": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "forestry:preview": "npm-run-all build:*"
  },
  "keywords": [],
  "author": "Anthony Warnick",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.2",
    "parcel": "^2.10.3",
    "sass": "^1.69.5",
    "sharp": "^0.33.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajwarnick/ajwarnick.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/ajwarnick/ajwarnick.github.io/issues"
  },
  "homepage": "https://github.com/ajwarnick/ajwarnick.github.io#readme",
  "dependencies": {
    "list.js": "^2.3.1",
    "photoswipe": "^5.3.7",
    "tachyonjs": "^2.0.1",
    "wavesurfer.js": "^6.1.0"
  }
}
